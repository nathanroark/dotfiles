{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "root_icon": "\uf292"
          },
          "style": "diamond",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "foreground": "#7FB4CA",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\u250f[<#98BB6C>{{ .Path }}</>]",
          "type": "path"
        },
        {
          "foreground": "#FFA066",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "[<#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#ffffff>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\ueb4b</> {{ .StashCount }}{{ end }}]",
          "type": "git"
        },
        {
          "foreground": "#E46876",
          "style": "plain",
          "template": "[{{ .Profile }}{{if .Region}}@{{ .Region }}{{ end }}]",
          "type": "aws"
        },
        {
          "foreground": "#E46876",
          "style": "plain",
          "template": "[{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}]",
          "type": "kubectl"
        },
        {
          "foreground": "#E46876",
          "style": "plain",
          "template": "[\uf0e7]",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#7FB4CA",
          "style": "plain",
          "template": "\u2516[\u2500Î”",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
