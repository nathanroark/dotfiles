    "template": "Mocha"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .WSL }}WSL in {{ end }}{{.Icon}} ",
          "foreground": "p:Lavender",
          "background": "p:Crust",
          "type": "os",
          "style": "powerline",
          "foreground_templates": [
            "{{ if .Root }}p:Pink{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "max_depth": 2,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:Crust",
          "powerline_symbol": "\ue0b4",
          "background": "p:Lavender",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ",
          "type": "project",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .Segments.Project.Name }}{{ .Segments.Project.Name }}{{ else }}{{ .Segments.Git.RepoName }}{{ end }}",
          "foreground": "p:Crust",
          "powerline_symbol": "\ue0b4",
          "background": "p:Flamingo",
          "type": "text",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_working_count": true
          },
          "foreground": "p:Crust",
          "powerline_symbol": "\ue0b4",
          "background": "p:Rosewater",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:Yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:Maroon{{ end }}",
            "{{ if gt .Ahead 0 }}p:Peach{{ end }}",
            "{{ if gt .Behind 0 }}p:Peach{{ end }}"
          ],
          "templates": [
            " {{ .HEAD }}",
            "{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}",
            "{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}",
            "{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}",
            "{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}",
            "{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}",
            "{{ if gt .WorktreeCount 0 }} \uf1bb {{ .WorktreeCount }}{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin"
          },
          "foreground": "p:Crust",
          "powerline_symbol": "\ue0b6",
          "background": "p:Pink",
          "type": "executiontime",
          "style": "powerline",
          "background_templates": [
            "{{ if gt .Code 0 }}p:Red{{ end }}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "p:Crust",
          "powerline_symbol": "\ue0b6",
          "background": "p:Mauve",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "p:Green",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:Red{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
